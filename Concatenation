
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    scanf("%d",&n);
    int a[n];
    
    int b[2*n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
        
        b[i] = a[i];
        b[i+n] = a[i];}
  
    for(int i=0;i<2*n;i++){
        printf("%d ",b[i]);
    }
    return 0;
}
